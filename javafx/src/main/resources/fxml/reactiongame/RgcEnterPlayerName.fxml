<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="585.0" maxWidth="1040.0" minHeight="585.0" minWidth="1040.0"
  xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="de.hhn.it.devtools.javafx.controllers.reactiongame.RgcEnterPlayerNameController">
  <Label alignment="CENTER" layoutX="304.0" layoutY="119.0" prefHeight="113.0" prefWidth="433.0"
    text="Enter name:" textAlignment="CENTER">
    <font>
      <Font name="Arial Black" size="58.0"/>
    </font>
  </Label>
  <TextField fx:id="nameTf" alignment="CENTER" layoutX="338.0" layoutY="232.0" prefHeight="63.0"
    prefWidth="368.0" promptText="your name">
    <font>
      <Font name="Arial Black" size="37.0"/>
    </font>
  </TextField>
  <Button layoutX="378.0" layoutY="427.0" mnemonicParsing="false" onAction="#onContinueBtn"
    prefHeight="71.0" prefWidth="287.0"
    style="-fx-background-color: #BCEE68; -fx-border-color: black; -fx-border-width: 4;"
    text="Continue">
    <font>
      <Font name="Arial Black" size="23.0"/>
    </font>
  </Button>
</AnchorPane>
