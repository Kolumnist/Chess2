<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<fx:root maxHeight="480" maxWidth="680" minHeight="480" minWidth="680" prefHeight="400.0"
         prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" type="AnchorPane">
   <children>
      <Label fx:id="playerName" text="Playername" />
      <Label fx:id="score" layoutX="286.0" text="Label" />
      <Button fx:id="openInventory" layoutX="530.0" layoutY="1.0" mnemonicParsing="false"
              onAction="#openInventoryAction" prefHeight="25.0" prefWidth="70.0" text="Inventory" />
      <ChoiceBox fx:id="directionChoiceBox" layoutX="450.0" layoutY="127.0" prefWidth="150.0" />
      <TextField fx:id="playerTextField" layoutX="31.0" layoutY="72.0" prefHeight="90.0" prefWidth="240.0" />
      <TextField fx:id="roomTextField" layoutX="31.0" layoutY="175.0" prefHeight="90.0" prefWidth="240.0" />
      <TextField fx:id="actionTextField" layoutX="31.0" layoutY="278.0" prefHeight="90.0" prefWidth="240.0" />
      <Button fx:id="interactionButton" layoutX="528.0" layoutY="373.0" mnemonicParsing="false"
              onAction="#interactionAction" text="Interaction" />
      <Button fx:id="searchButton" layoutX="454.0" layoutY="373.0" mnemonicParsing="false"
              onAction="#searchAction" prefHeight="25.0" prefWidth="70.0" text="Search" />
      <Button fx:id="inspectButton" layoutX="530.0" layoutY="336.0" mnemonicParsing="false"
              onAction="#inspectAction" prefHeight="25.0" prefWidth="70.0" text="Inspect" />
      <Button fx:id="moveButton" layoutX="452.0" layoutY="336.0" mnemonicParsing="false"
              onAction="#moveAction" prefHeight="25.0" prefWidth="73.0" text="Move" />
   </children>
</fx:root>
