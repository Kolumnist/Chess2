<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="gameScreenAnchorPane" maxHeight="680.0" maxWidth="1080.0" minHeight="680.0" minWidth="1080.0" prefHeight="680.0" prefWidth="1080.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="de.hhn.it.devtools.javafx.controllers.memory.MemoryGameController">
   <children>
      <Label fx:id="timeTextLabel" layoutX="899.0" layoutY="23.0" text="Zeit:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="timeDisplayLabel" layoutX="970.0" layoutY="23.0" text="0:00 min">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="optionsButton" layoutX="54.0" layoutY="599.0" mnemonicParsing="false" onAction="#onOptionsButtonClicked" prefHeight="38.0" prefWidth="132.0" text="Optionen">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="finishButton" layoutX="901.0" layoutY="596.0" mnemonicParsing="false" onAction="#onFinishButtonClicked" text="Beenden">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
